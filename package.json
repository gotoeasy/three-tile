{
	"name": "three-tile",
	"private": false,
	"version": "0.10.0",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./packages/lib/dist/three-tile.umd.cjs",
	"module": "./packages/lib/dist/three-tile.js",
	"types": "./packages/lib/dist/three-tile.d.ts",
	"exports": {
		".": {
			"import": "./packages/lib/dist/three-tile.js",
			"require": "./packages/lib/dist/three-tile.umd.cjs",
			"types": "./packages/lib/dist/three-tile.d.ts"
		},
		"./plugin": {
			"import": "./packages/plugin/dist/three-tile-plugin.js",
			"require": "./packages/plugin/dist/three-tile-plugin.umd.cjs",
			"types": "./packages/plugin/dist/three-tile-plugin.d.ts"
		}
	},
	"author": {
		"name": "GuoJF",
		"email": "hz_gjf@163.com"
	},
	"scripts": {
		"dev": "npm run dev --workspace=packages/demo",
		"build:lib": "npm run build --workspace=packages/lib",
		"build:plugin": "npm run build --workspace=packages/plugin",
		"build:demo": "npm run build --workspace=packages/demo",
		"build": "npm run build:lib && npm run build:plugin",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"**/*.{ts,js,json,md}\""
	},
	"devDependencies": {
		"@types/node": "^20.17.30",
		"@types/offscreencanvas": "^2019.7.0",
		"@types/three": "^0.165.0",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"typedoc": "^0.25.13",
		"typescript": "^5.3.3",
		"vite": "^5.4.16",
		"vite-plugin-dts": "^4.0.3"
	},
	"peerDependencies": {
		"three": "^0.165.0"
	},
	"workspaces": [
		"packages/*"
	],
	"description": "A lightweight tile map using threejs",
	"license": "GPL-3.0",
	"keywords": [
		"three",
		"gis",
		"tile",
		"map",
		"3D",
		"cesium"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sxguojf/three-tile.git"
	},
	"bugs": {
		"url": "https://github.com/sxguojf/three-tile/issues"
	},
	"homepage": "https://sxguojf.github.io/three-tile-doc",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	}
}
